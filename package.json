    "next": "12.0.10",
    "next-with-less": "^2.0.5",
    "prettier": "^2.6.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/node": "17.0.18",
    "@types/react": "17.0.39",
    "eslint-config-next": "12.0.10",
